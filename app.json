{
  "expo": {
    "name": "WolFlex",
    "slug": "WolFlex",
    "assets": ["./assets/fonts"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/WolfAlone.png",
    "scheme": "wolflex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/WolfAlone.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/WolfAlone.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundImage": "./assets/images/WolfAlone.png",
          "resizeMode": "contain",
          "imageWidth": 300,
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
